<table class="ui celled table">
<thead>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Action</th>
    </tr>
  </thead>
  <tbody>
  {{#each actors}}
    <tr>
      <td>{{this.first_name}}</td>
        <td>{{this.last_name}}</td>
        <td>
            <a class="ui teal basic button" href="/actors/{{this.id}}/edit">Edit</a>
        </td>
    </tr>
    {{/each}}
    </tbody>
</table>